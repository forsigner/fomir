{
  "compilerOptions": {
    "module": "esnext",
    "lib": [
      "dom",
      "esnext",
      "ES2020"
    ],
    "target": "ESNext",
    "importHelpers": true,
    "declaration": true,
    "sourceMap": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "jsx": "react",
    "esModuleInterop": true
  }
}